{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
    "implicit-arrow-linebreak": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
